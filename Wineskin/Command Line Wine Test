#!/bin/bash
echo "";echo ""
cd "$(dirname "$0")"
export PATH="$PWD:$PATH"
cd "$(dirname "$0")/../../../Contents/Frameworks/wswine.bundle/bin"
export PATH="$PWD:$PATH"
cd "$(dirname "$0")/../../../Contents/Frameworks/wswine.bundle/lib"
export DYLD_FALLBACK_LIBRARY_PATH="$PWD:/opt/X11/lib:/opt/local/lib:/usr/lib:/usr/libexec:/usr/lib/system:/usr/X11/lib:/usr/X11R6/lib"
cd "$(dirname "$0")/../../../Contents/Frameworks"
export DYLD_FALLBACK_LIBRARY_PATH="/opt/X11/lib:/opt/local/lib:/usr/lib:/usr/libexec:/usr/lib/system:/usr/X11/lib:/usr/X11R6/lib:$PWD"
export PS1="\nWineskin Command Line Wine Test\n\h:\W \u$ "
cd "$HOME"
echo "This is for testing normal Wine use only."
echo ""
echo "Default Wineprefix is ~/.wine and NOT the wrapper!"
echo ""
echo "If you are using a stock Wine engine then this is"
echo "    just like using normal command line Wine"
echo ""
echo "Winetricks should also be available"
echo ""
/bin/bash